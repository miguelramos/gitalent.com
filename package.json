{
  "name": "gitalent-site",
  "version": "0.0.0",
  "description": "The gitalent.com website on angular 2 framework.",
  "main": "main.js",
  "scripts": {
    "docs": "./node_modules/.bin/compodoc -p tsconfig.json && ./node_modules/.bin/compodoc -s",
    "build:dll": "npm run clean:dll && ./node_modules/.bin/webpack",
    "build:aot": "npm run build:aot:prod",
    "build:aot:prod": "npm run clean:dist && npm run sass && ./node_modules/.bin/webpack",
    "compile": "npm run compile:aot && npm run clean:compile",
    "compile:aot": "npm run sass && npm run clean:compile && npm run ngc && npm run build:aot",
    "clean:dll": "./node_modules/.bin/rimraf build",
    "clean:dist": "./node_modules/.bin/rimraf dist .awcache",
    "clean:compile": "./node_modules/.bin/rimraf \"src/compile\" ",
    "changelog": "conventional-changelog -i CHANGELOG.md -s -p angular -v",
    "dev": "./node_modules/.bin/webpack-dev-server",
    "ngc": "./node_modules/.bin/ngc -p tsconfig.aot.json",
    "check:npm": "./node_modules/.bin/npmvet -r inlinetable",
    "postinstall": "npm run sass && npm run build:dll",
    "start": "webpack-dashboard -c blue -- npm run server:dev",
    "start:simple": "npm run server:dev",
    "sass": "./node_modules/.bin/node-sass src/assets/scss/styles.scss -o src/assets/css  --include-path node_modules --output-style compressed -q",
    "sass:watch": "./node_modules/.bin/node-sass -w src/assets/scss -o src/assets/css --include-path node_modules --output-style compressed -q",
    "server:dev": "npm-run-all -p -r dev sass:watch",
    "server:dev:hmr": "npm run server:dev -- --inline --hot",
    "server:prod": "cd dist/client && python -m SimpleHTTPServer",
    "test:once": "karma start",
    "test:once:ci": "karma start",
    "test": "npm run test:once",
    "lint": "./node_modules/.bin/tslint \"src/app/**/*.ts\" "
  },
  "dependencies": {
    "@angular/common": "2.4.6",
    "@angular/compiler": "2.4.6",
    "@angular/compiler-cli": "2.4.6",
    "@angular/core": "2.4.6",
    "@angular/forms": "2.4.6",
    "@angular/http": "2.4.6",
    "@angular/platform-browser": "2.4.6",
    "@angular/platform-browser-dynamic": "2.4.6",
    "@angular/platform-server": "2.4.6",
    "@angular/router": "3.4.6",
    "@angular/tsc-wrapped": "0.5.2",
    "@angularclass/hmr": "1.2.2",
    "@angularclass/hmr-loader": "3.0.2",
    "@types/dotenv": "^2.0.20",
    "core-decorators": "^0.15.0",
    "core-js": "2.4.1",
    "reflect-metadata": "^0.1.9",
    "rxjs": "^5.1.0",
    "zone.js": "^0.7.6"
  },
  "devDependencies": {
    "@types/body-parser": "~0.0.33",
    "@types/compression": "~0.0.33",
    "@types/cookie-parser": "~1.3.30",
    "@types/core-decorators": "^0.10.30",
    "@types/core-js": "~0.9.35",
    "@types/express": "~4.0.35",
    "@types/express-serve-static-core": "~4.0.40",
    "@types/hammerjs": "~2.0.34",
    "@types/ip": "~0.0.29",
    "@types/jasmine": "~2.5.41",
    "@types/lodash": "^4.14.52",
    "@types/mime": "~0.0.29",
    "@types/node": "~7.0.4",
    "@types/selenium-webdriver": "~2.44.29",
    "@types/serve-static": "~1.7.31",
    "angular-pipes": "^5.8.2",
    "angular2-router-loader": "^0.3.4",
    "angular2-template-loader": "^0.6.0",
    "awesome-typescript-loader": "~3.0.0-beta.18",
    "bulma": "^0.3.1",
    "codelyzer": "~2.0.0-beta.4",
    "compodoc": "0.0.38",
    "compression-webpack-plugin": "~0.3.2",
    "conventional-changelog-cli": "^1.2.0",
    "copy-webpack-plugin": "~4.0.1",
    "es6-promise": "~4.0.5",
    "es6-promise-loader": "~1.0.2",
    "font-awesome": "^4.7.0",
    "hammerjs": "~2.0.8",
    "html-webpack-harddisk-plugin": "0.0.2",
    "html-webpack-plugin": "~2.28.0",
    "imports-loader": "~0.7.0",
    "ip": "~1.1.4",
    "istanbul-instrumenter-loader": "~1.2.0",
    "jasmine": "^2.5.3",
    "jasmine-core": "^2.5.2",
    "json-loader": "~0.5.4",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
