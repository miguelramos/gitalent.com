    "karma-phantomjs-launcher": "~1.0.2",
    "karma-remap-coverage": "~0.1.4",
    "karma-sourcemap-loader": "~0.3.7",
    "karma-webpack": "~2.0.2",
    "lodash": "^4.17.4",
    "ng2-translate": "^5.0.0",
    "node-sass": "~4.5.0",
    "npm-run-all": "^4.0.1",
    "npmvet": "^0.1.2",
    "phantomjs-polyfill": "~0.0.2",
    "phantomjs-prebuilt": "~2.1.14",
    "protractor": "~4.0.14",
    "raw-loader": "^0.5.1",
    "rimraf": "~2.5.4",
    "sass-mediaqueries": "^1.6.1",
    "source-map-loader": "~0.1.6",
    "store": "^1.3.20",
    "string-replace-loader": "~1.0.5",
    "to-string-loader": "~1.1.5",
    "ts-helpers": "^1.1.2",
    "ts-node": "~2.0.0",
    "tslint": "~4.4.2",
    "tslint-loader": "~3.3.0",
    "typescript": "~2.1.5",
    "verbal-expressions": "^0.3.0",
    "watch-run": "~1.2.5",
    "webpack": "^2.2.1",
    "webpack-dashboard": "^0.3.0",
    "webpack-dev-middleware": "~1.10.0",
    "webpack-dev-server": "^2.3.0",
    "webpack-merge": "~2.6.1"
  },
  "repository": {
    "type": "git",
